{
    "name": "team-durumi/kyeol",
    "description": "일본군'위안부'문제연구소 웹진 결",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "yeongtaek, Hong",
            "email": "mozodev@users.noreply.github.com",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "composer/installers": "^1.2",
        "composer/semver": "^1.4",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/preserve-paths": "^0.1",
        "drupal/admin_menu": "*",
        "drupal/adminimal_admin_menu": "*",
        "drupal/adminimal_theme": "^1.26",
        "drupal/advanced_help": "*",
        "drupal/autotag": "^3.1",
        "drupal/backup_migrate": "*",
        "drupal/ckeditor": "*",
        "drupal/composer_autoloader": "^1.0",
        "drupal/conditional_styles": "*",
        "drupal/context": "*",
        "drupal/ctools": "*",
        "drupal/devel": "*",
        "drupal/drupal": "^7.62",
        "drupal/elysia_cron": "*",
        "drupal/entity": "*",
        "drupal/file_entity": "*",
        "drupal/geofield": "*",
        "drupal/geofield_gmap": "*",
        "drupal/geophp": "*",
        "drupal/google_analytics": "*",
        "drupal/google_analytics_reports": "*",
        "drupal/i18n": "*",
        "drupal/imagefield_crop": "*",
        "drupal/jcaption": "*",
        "drupal/l10n_update": "*",
        "drupal/libraries": "*",
        "drupal/link": "*",
        "drupal/linkit": "*",
        "drupal/media": "*",
        "drupal/media_ckeditor": "*",
        "drupal/menu_block": "*",
        "drupal/menu_breadcrumb": "*",
        "drupal/metatag": "*",
        "drupal/module_filter": "*",
        "drupal/multiform": "*",
        "drupal/nodereference_selector": "*",
        "drupal/nodereference_url": "*",
        "drupal/panels": "*",
        "drupal/pathauto": "*",
        "drupal/plupload": "*",
        "drupal/references": "*",
        "drupal/search_api": "*",
        "drupal/search_api_db": "*",
        "drupal/term_reference_tree": "*",
        "drupal/token": "*",
        "drupal/variable": "*",
        "drupal/video": "*",
        "drupal/views": "*",
        "drupal/xmlsitemap": "*",
        "drupal/youtube": "*",
        "drush/drush": "^8.0",
        "symfony/filesystem": "~2.7|^3",
        "webflo/drupal-finder": "^1.0.0"
    },
    "conflict": {
        "drupal/core": "8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "DrupalProject\\composer\\ScriptHandler::removeInternalFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/": ["type:drupal-core"],
            "web/profiles/{$name}/": ["type:drupal-profile"],
            "web/sites/all/drush/{$name}/": ["type:drupal-drush"],
            "web/sites/all/libraries/{$name}/": ["type:drupal-library"],
            "web/sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
            "web/sites/all/themes/contrib/{$name}/": ["type:drupal-theme"]
        },
        "patches": {
            "cweagans/composer-patches": {
                "Call the preserve paths hooks" : "https://github.com/SebCorbin/composer-patches/commit/1cb9bacba51f8bba0c5f3f322c3bb61e4ceae974.patch"
            },
            "drupal/ckeditor": {
                "Add %conf_path% and %plugin_module_path% to plugin path render placeholders/replacements to make features reusable": "https://www.drupal.org/files/issues/ckeditor-plugin_module_paths-2422875-34.patch"
            }
        },
        "preserve-paths": [
            "web/sites"
        ]
    }
}
